{
  "name": "restful-model",
  "version": "0.0.13",
  "description": "An Isomorphic ORM for rest APIs",
  "main": "lib/index.js",
  "browser": "dist/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha test/*",
    "build": "npx webpack",
    "lint": "npx eslint lib/*.js test/*js",
    "lint:fix": "npm run lint -- --fix",
    "version:inc": "npm version patch && git push && git push --tags",
    "npm:pub": "npm publish --registry=https://registry.npmjs.org/ --access=public",
    "pub": "npm run version:inc && npm run npm:pub"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/faboulaws/restful-model.git"
  },
  "keywords": [
    "rest",
    "orm",
    "js"
  ],
  "author": "Lod Lawson <faboulaws@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.0.1",
    "nock": "^9.1.9",
    "sinon": "^4.4.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "isomorphic-fetch": "^2.2.1"
  }
}
